<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Petclinic</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/lib/bootstrap-icons/font/bootstrap-icons.css"/>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.css">
    <link rel="stylesheet" href="~/lib/Swiper/swiper-bundle.css"/>
    <link rel="stylesheet" href="~/lib/toastr.js/toastr.css" />
    <link rel="stylesheet" href="~/lib/limonte-sweetalert2/sweetalert2.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>

</head>
<body>
<header>
    <partial name="~/Views/Partials/_Nav.cshtml"/>
</header>
<main role="main">
    @RenderBody()
</main>

<partial name="~/Views/Partials/_Footer.cshtml"/>

<script src="~/lib/jquery/jquery.min.js"></script>
<partial name="~/Views/Shared/_ValidationScriptsPartial.cshtml"/>
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.js"></script>
<script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/Swiper/swiper-bundle.js"></script>
<script src="~/lib/limonte-sweetalert2/sweetalert2.js"></script>
<script src="https://cdn.tiny.cloud/1/527rqeet03jtxqf9cb870hu7iz6ol12k54ld4w6gol1rl4oa/tinymce/6/tinymce.min.js" referrerpolicy="origin"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script>
    $(document).ready(function() {
        $('ul.navbar-nav').find('a[href="' + location.pathname + '"]')
            .closest('li').addClass('active');
    });
</script>
@await RenderSectionAsync("Scripts", required: false)
</body> 
</html>